{
    "name" : "12306buy",
    "version" : "1.0",
    "manifest_version" : 2,
    "description" : "12306buy plugin",
    "icons": {
        "128": "logo.png"
    },
    "browser_action" : {
        "default_icon" : "logo.png",
        "default_title" : "12306buy",
        "default_popup" : "popup.html"
    },
    "permissions" : [
        "tabs",
        "http://*/*",
        "https://*/*",
        "notifications"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}